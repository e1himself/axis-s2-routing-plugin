{
  "name": "axis/axis-s2-routing-plugin",
  "description": "symfony 1.x plugin that integrates Symfony2 routing component into your application",

  "type": "symfony1-plugin",
  "extra": {
    "installer-name": "AxisS2RoutingPlugin"
  },

  "keywords": ["symfony", "routing", "plugin", "symfony2 routing"],
  "license": "MIT",
  "homepage": "https://github.com/e1himself/axis-s2-routing-plugin",

  "authors": [
    {
      "name": "Ivan Voskoboynyk",
      "email": "ioann.voskoboynyk@gmail.com",
      "homepage": "https://github.com/e1himself"
    }
  ],
  "version": "0.1.0",

  "autoload": {
    "psr-0": {
      "Axis\\S1\\S2Routing": "lib/"
    }
  },

  "require": {
    "php": ">=5.3.0",
    "composer/installers": "*",
    "symfony/routing": "2.1.x-dev",
    "symfony/config": "2.1.x-dev"
  }
}
